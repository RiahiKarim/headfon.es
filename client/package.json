{
  "name": "client",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@urql/exchange-suspense": "^0.1.0",
    "css-vars": "^2.2.0",
    "graphql": "^14.5.8",
    "node-sass-chokidar": "^1.4.0",
    "react": "16.12.0",
    "react-copy-write": "^0.8.0",
    "react-dom": "16.12.0",
    "react-player-controls": "^1.1.0",
    "urql": "^1.7.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path src/styling --include-path node_modules",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path src/styling --include-path node_modules --watch --recursive",
    "start": "react-scripts start",
    "build": "yarn build-css && react-scripts build && sw-precache --root='build/' --config config/sw.js ",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-copy-write": "^0.7.1",
    "@types/react-dom": "^16.9.4",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "react-scripts": "^3.3.0",
    "sw-precache": "^5.2.1",
    "typescript": "^3.7.4"
  },
  "proxy": "http://localhost:3004/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
